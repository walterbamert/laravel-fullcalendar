{
  "name": "walterbamert/laravel-fullcalendar",
  "description": "Updated Laravel component for fullcalendar package base on Edo Freriks original package",
  "keywords": [
    "fullcalendar",
    "javascript",
    "event",
    "calendar",
    "laravel"
  ],
  "homepage": "https://github.com/walterbamert/laravel-fullcalendar",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    },
    {
      "name": "Walter Bamert",
      "email": "walter@autoitinc.com"
    }
  ],
  "require": {
    "php": ">=^7.2.5.*",
    "laravel/framework": "^7.*",
    "npm-asset/fullcalendar": "^5.*"
  },
  "extra": {
    "npm": {
      "fullcalendar": "^5.*"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^5.*",
    "phpunit/phpunit": "^5.*"
  },
  "autoload": {
    "psr-4": {
      "SMPT\\Fullcalendar\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SMPT\\Fullcalendar\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SMPT\\Fullcalendar\\FullcalendarServiceProvider"
      ],
      "aliases": {
        "Fullcalendar": "SMPT\\Fullcalendar\\Facades\\Fullcalendar"
      }
    }
  }
}
