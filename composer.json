{
  "name": "walterbamert/laravel-fullcalendar",
  "description": "Updated Laravel component for fullcalendar package base on Edo Freriks original package",
  "keywords": [
    "fullcalendar",
    "javascript",
    "event",
    "calendar",
    "laravel"
  ],
  "homepage": "https://github.com/walterbamert/laravel-fullcalendar",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    },
    {
      "name": "Walter Bamert",
      "email": "walter@autoitinc.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "laravel/framework": "7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "extra": {
    "npm": {
      "fullcalendar": "^5.*"
    }
  },
  "autoload": {
    "psr-4": {
      "walterbamert\\Fullcalendar\\": "src"
    }
  },
  "autoload-dev": {
